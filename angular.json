      "root": "projects/topomojo-mks",
      "sourceRoot": "projects/topomojo-mks/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/topomojo-mks",
            "index": "projects/topomojo-mks/src/index.html",
