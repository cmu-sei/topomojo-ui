{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "topomojo-work": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/topomojo-work",
      "sourceRoot": "projects/topomojo-work/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": {
              "base": "dist/topomojo-work"
            },
            "index": "projects/topomojo-work/src/index.html",
            "polyfills": ["zone.js"],
            "tsConfig": "projects/topomojo-work/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/topomojo-work/src/favicon.ico",
              "projects/topomojo-work/src/assets",
              {
                "glob": "min/**/*",
                "input": "node_modules/monaco-editor",
                "output": "assets/monaco"
              },
              {
                "glob": "**/*",
                "input": "node_modules/@cmusei/console-forge/assets",
                "output": "assets"
              }
            ],
            "styles": [
              "projects/topomojo-work/src/styles.scss",
              "node_modules/vmware-wmks/wmks-all.css"
            ],
            "scripts": [
              "node_modules/marked/marked.min.js",
              "node_modules/emoji-toolkit/lib/js/joypixels.min.js",
              "node_modules/jquery/dist/jquery.js",
              "node_modules/jquery-ui-dist/jquery-ui.js",
              "node_modules/vmware-wmks/wmks.min.js"
            ],
            "stylePreprocessorOptions": {
              "sass": {
                "silenceDeprecations": [
                  "mixed-decls",
                  "color-functions",
                  "global-builtin",
                  "import",
                  "abs-percent"
                ]
              }
            },
            "browser": "projects/topomojo-work/src/main.ts"
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "1mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/topomojo-work/src/environments/environment.ts",
                  "with": "projects/topomojo-work/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "topomojo-work:build:production"
            },
            "development": {
              "buildTarget": "topomojo-work:build:development",
              "port": 4201
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "topomojo-work:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/topomojo-work/src/test.ts",
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "projects/topomojo-work/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/topomojo-work/src/favicon.ico",
              "projects/topomojo-work/src/assets"
            ],
            "styles": ["projects/topomojo-work/src/styles.scss"],
            "scripts": []
          }
        }
      }
    },
    "topomojo-mks": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
